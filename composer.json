{
    "name": "yahaaylabs/laravel-barangay-search",
    "description": "A Laravel Livewire component for searching Philippine Barangays with Mary UI support",
    "type": "library",
    "keywords": [
        "laravel",
        "livewire",
        "barangay",
        "philippines",
        "mary-ui",
        "gis"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gerald Villacarlos",
            "email": "gerald@yahaay.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "yahaaylabs/gis.ph-sdk-php": "^0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "YahaayLabs\\LaravelBarangaySearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YahaayLabs\\LaravelBarangaySearch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YahaayLabs\\LaravelBarangaySearch\\BarangaySearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}